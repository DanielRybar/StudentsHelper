<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:conv="clr-namespace:StudentsHelper.Converters">

    <!-- converters -->
    <conv:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    <conv:DateTimeToFormattedStringConverter x:Key="DateTimeToFormattedStringConverter" />

    <!-- strings -->
    <x:String x:Key="LoadingData">Načítání dat</x:String>
    <x:String x:Key="SavingData">Ukládání dat</x:String>
    <x:String x:Key="DeletingData">Mazání dat</x:String>

</ResourceDictionary>